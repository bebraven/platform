<p id="notice"><%= notice %></p>

<h1>Versions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Updated By</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_content_histories.reverse.each do |course_content_history| %>
      <tr>
        <td><%= course_content_history.created_at %></td>
        <td><%= course_content_history.title %></td>
        <td><%= course_content_history.user.email %></td>
        <td><%= course_content_history.body %></td>
        <td><%= link_to "#{course_content_history.id}", [@course_content, course_content_history] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
